#contentW
  include ./rightPane.pug
  #metaW 
    #metaC
      #meta(data-focused="true")
        .name 
          input(placeholder="Table name (ex: Pets)")
        .description 
          input(placeholder="Table name (ex: Animals adopted by humans)")
  #tableW
    #tableC 
      #table
        table 
          thead 
            tr 
              th(data-line_num="1" data-field_num="0" data-focused="false" data-type="array")#rowCol &nbsp; 
              th(data-line_num="3" data-field_num="0" data-focused="false" data-type="key") col1 
              th(data-line_num="4" data-field_num="0" data-focused="false" data-type="key") col2 
              th(data-line_num="5" data-field_num="0" data-focused="false" data-type="key") col3
          tbody 
            tr 
              td(data-focused="false" data-line_num="2", data-field_num="0" data-type="object").rowNum 1
              td(data-focused="false" data-line_num="3", data-field_num="2" data-type="value") val1
              td(data-focused="false" data-line_num="4", data-field_num="2" data-type="value") val2
              td(data-focused="false" data-line_num="5", data-field_num="2" data-type="value") val3
            tr 
              td(data-focused="false" data-line_num="7", data-field_num="0" data-type="object").rowNum 2
              td(data-focused="false" data-line_num="3", data-field_num="2" data-type="value") val4
              td(data-focused="false" data-line_num="4", data-field_num="2" data-type="value") val5
              td(data-focused="false" data-line_num="5", data-field_num="2" data-type="value") val6

